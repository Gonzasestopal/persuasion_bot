DATABASE_URL=postgresql://postgres:supersecret@db/db OR postgresql://postgres:supersecret@localhost:5432/db
OPENAI_API_KEY=yourapikey
LLM_PROVIDER='openai'
DIFFICULTY='medium'
ANTHROPIC_API_KEY=yourapikey

## optional

HISTORY_LIMIT=5
EXPIRES_MINUTES=60
REQUEST_TIMEOUT_S=25
POOL_MIN=1
POOL_MAX=10
LLM_MODEL='gpt-o4'
USE_INMEMORY_REPO=False
DISABLE_DB_POOL=False
ANTHROPIC_API_KEY=key

# Fallback selection
PRIMARY_LLM=openai
SECONDARY_LLM=anthropic

# Behavior
LLM_TEMPERATURE=0.3
LLM_PER_PROVIDER_TIMEOUT_S=12

MIN_ASSISTANT_TURNS_BEFORE_VERDICT=5
REQUIRED_POSITIVE_JUDGEMENTS=2
MAX_OUTPUT_TOKENS=120
